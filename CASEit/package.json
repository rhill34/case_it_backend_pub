  "private": true,
  "description": "CASEit Serverless REST API: Babel, Cloud Functions, Firebase, Webpack, etc --RH3 ",
  "scripts": {
    "start: ex": "node ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve: web": "webpack-dev-server --mode development --output-public-path=/build/",
